{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "borswer": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss",
                    "skipTests": true
                },
                "@schematics/angular:class": {
                    "skipTests": true
                },
                "@schematics/angular:directive": {
                    "skipTests": true
                },
                "@schematics/angular:guard": {
                    "skipTests": true
                },
                "@schematics/angular:interceptor": {
                    "skipTests": true
                },
                "@schematics/angular:module": {
                    "skipTests": true
                },
                "@schematics/angular:pipe": {
                    "skipTests": true
                },
                "@schematics/angular:service": {
                    "skipTests": true
                },
                "@schematics/angular:application": {
                    "strict": true
                }
            },
            "root": "",
            "sourceRoot": "src/render",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@miup/ngx-electron-builder:build",
                    "options": {
                        "outputPath": "dist/borswer",
                        "index": "src/render/index.html",
                        "main": "src/render/main.ts",
                        "polyfills": "src/render/polyfills.ts",
                        "tsConfig": "tsconfig.render.json",
                        "aot": true,
                        "assets": [
                            "src/render/favicon.ico",
                            "src/render/assets"
                        ],
                        "styles": [
                            "src/render/styles.scss"
                        ],
                        "scripts": [],
                        "mainProcess": "src/main/index.ts",
                        "mainProcessOutputName": "index.js",
                        "mainProcessTsConfig": "tsconfig.main.json"
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/render/environments/environment.ts",
                                    "with": "src/render/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@miup/ngx-electron-builder:dev-server",
                    "options": {
                        "browserTarget": "borswer:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "borswer:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "borswer:build"
                    }
                }
            }
        }
    },
    "cli": {
      "analytics": false

    },
    "defaultProject": "borswer"
}
